<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.fitlog">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/deadliftlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddExercises" />
        <activity android:name=".WorkoutHistory" />
        <activity android:name=".GraphScreen" />
        <activity android:name=".HistorySelectionScreen" />
        <activity android:name=".SelectMuscleGroup" />
        <activity android:name=".WorkoutOverview"></activity>
    </application>

</manifest>